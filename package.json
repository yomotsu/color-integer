{
  "name": "hex-color-utils",
  "version": "0.0.3",
  "author": "Yomotsu",
  "license": "MIT",
  "repository": "yomotsu/hex-color-utils",
  "jsnext:main": "dist/hex-color-utils.module.js",
  "module": "dist/hex-color-utils.module.js",
  "main": "dist/hex-color-utils.js",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "rollup": "^0.65.0",
    "rollup-plugin-typescript2": "^0.16.1",
    "rollup-watch": "^4.3.1",
    "typescript": "^3.0.1",
    "uglify-js": "^3.4.8"
  },
  "scripts": {
    "dev": "npm run watch",
    "watch": "rollup --config --watch",
    "build": "rollup --config && uglifyjs dist/hex-color-utils.js --comments '/^/*!/' > dist/hex-color-utils.min.js"
